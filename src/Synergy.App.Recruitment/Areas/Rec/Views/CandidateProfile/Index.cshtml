    function OnCandExpBound() {
                        $.ajax({
                    url:'@Url.Action("GetCandidateExperienceDuration", "CandidateProfile", new { @area= "Recruitment" })?candidateProfileId=@Model.Id',
                    type: "GET",
                    contentType: "application/json",
                    dataType: "JSON",
                    success: function (response) {
                        if (response.success) {
                            var twe = response.data;
                            $("#TotalWorkExperienceYear").val(twe);
                        }
                    }
                    });
    }

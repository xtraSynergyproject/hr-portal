            win.OpenWindow({ Title: 'View Task Details', Width: 1200, Height: 1200 });
            return false;
        }*@

        //Inprogress
        var dataSource = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsTask/ReadTaskDataInProgress?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&requestby=" + '@ViewBag.reqType'+'&portalNames='+'@ViewBag.PortalNames',
                    dataType: "json"
                }
            },
        });

        $("#kgrdTask").kendoListView({
            dataSource: dataSource,
            dataBound: OnDataBoundPending,
            template: kendo.template($("#templateTask").html())
        });

        //Overdue
        var dataSourceOverdue = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsTask/ReadTaskDataOverdue?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&requestby=" + '@ViewBag.reqType'+'&portalNames='+'@ViewBag.PortalNames',
                    dataType: "json"
                }
            },
        });

        $("#kgrdTaskOverDue").kendoListView({
            dataSource: dataSourceOverdue,
            dataBound: OnDataBoundOverdue,
            template: kendo.template($("#templateTaskOverdue").html())
        });

        //Completed
        var dataSourceCom = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsTask/ReadTaskDataCompleted?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&requestby=" + '@ViewBag.reqType'+'&portalNames='+'@ViewBag.PortalNames',
                    dataType: "json"
                }
            },
        });

        $("#kgrdTaskCompleted").kendoListView({
            dataSource: dataSourceCom,
            dataBound: OnDataBoundCompleted,
            template: kendo.template($("#templateTaskCompleted").html())
        });

    });

    function FilterGridPending() {
        var search = {

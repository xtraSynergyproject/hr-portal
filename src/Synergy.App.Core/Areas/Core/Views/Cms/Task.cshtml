@using Synergy.App.ViewModel;
@using Synergy.App.Common;
@model TaskTemplateViewModel;
@{
    Layout = ViewBag.Layout;
}
@if (Model.ViewType == NtsViewTypeEnum.Classic)
{
    await Html.RenderPartialAsync("TaskDefault.cshtml", Model);
}
 
else
{
    await Html.RenderPartialAsync("TaskLightPage.cshtml", Model);
}
 

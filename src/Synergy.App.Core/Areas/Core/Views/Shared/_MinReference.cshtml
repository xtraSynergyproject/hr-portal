        function toTitleCase(str) {
            var arr = str.split('_');
            if (arr.length > 1) {
                str = arr[1].replaceAll('_', ' ');
            }
            else {
                str = str.replaceAll('_', ' ');
            }
            return str.replace(/\w\S*/g, function (txt) {
                return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
            });
        }
        function toTitleCaseGrid(str) {
            var arr = str.split('_');
            var newstr="";
            if (arr.length > 1) {
                newstr = arr.join(" ");
            }
            else {
                newstr = str.replaceAll('_', ' ');
            }
            return newstr.replace(/\w\S*/g, function (txt) {
                return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
            });
        }
        function ShowLoader(target) {
            if (target === undefined || target === null || target === 'undefined') {
                document.getElementById("global-overlay").style.display = "block";
                //$("#global-overlay").show();
                return;
            }
            kendo.ui.progress(target, true);
        }
        function HideLoader(target) {
            if (target === undefined || target === null || target === 'undefined') {
                document.getElementById("global-overlay").style.display = "none";
                //$("#global-overlay").hide();
                return;
            }
            kendo.ui.progress(target, false);
        }
        function refreshChart(chartId,param) {
            $.ajax({

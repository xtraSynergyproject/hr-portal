            var count = 38;
            if (menu.PageType===15) {
                count = 80;
            }
            var childs = menus0.filter(x => x.ParentId == menu.Id);
            if (childs !== null || childs.length > 0) {
                $.each(childs, function (key, value) {
                    count += GetMenuCount(value, menus0);
                });
            }
            return count;

          <TableCell Name="Cell407" Text="Blood report (Haemogram) which includes TLC, DLC, ESR, Haemoglobin, VDRL, Diabetics and blood grouping." Padding="2, 5, 2, 5" HorzAlign="Justify" Font="Arial, 12pt"/>
        </TableRow>
        <TableRow Name="Row128" Height="28.35" AutoSize="true" CanBreak="true">
          <TableCell Name="Cell408" Padding="2, 5, 2, 5" Font="Arial, 12pt"/>
          <TableCell Name="Cell409" Text="5." Padding="2, 5, 2, 5" Font="Arial, 12pt"/>

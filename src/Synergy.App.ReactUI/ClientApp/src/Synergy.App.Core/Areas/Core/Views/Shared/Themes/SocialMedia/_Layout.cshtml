        {
            @: refreshurl = '@ViewBag.Portal.Name?pageUrl=' + encodeURIComponent('pageId=' + id + '&portalId=' + portalId + '&pageType=' + pageType + '&source=' + source + '&dataAction=' + dataAction + '&recordId=' + recordId + '&cbm=' + cbm + '&templatecodes=' + templatecodes + '&lo=' + lo + '&pageName=' + pageName +'&popup=@Convert.ToBoolean(ViewBag.IsPopup)');
        }
        if (!popup) {
            history.pushState({}, null, refreshurl);
        }
        else {
            url += "&popup=true&lo=Popup";
        }
        LoadCmsPartialView(url, pageType, popup, w, h, title, cbm, reload, container, iframe)
        return false;


    function OnWebsiteClick(id, template, name) {
        $("li[data-page]").removeClass('active');
        $("li[data-page='" + name + "']").addClass('active');
        if (template == 'True') {
            $("#save-btn").hide();
            $("#save-as-btn").show();
            $("#gallery-as-btn").show();
            $("#glink-as-btn").hide();
        }
        else {

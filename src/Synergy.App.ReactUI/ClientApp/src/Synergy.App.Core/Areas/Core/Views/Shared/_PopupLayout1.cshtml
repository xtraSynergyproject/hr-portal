    function onSave() {

        kendo.ui.progress($('#vvveb-builder'), true);
        var content = $('#iframe1').contents().find("#HtmlContent").html();
        var style = $('#iframe1').contents().find("#vvvebjs-styles").html();
        var id = $('#iframe1').contents().find("#NoteId")[0].value;
        var url = "@Url.Action("ManageSynergyWebsite", "Editor", new { @area = "Cms" })";
        var model = { HtmlContent: content, NoteId: id, Style: style };
        if (id != null && id != "" && id != undefined) {
            $.post(url, model, function (res) {
                if (res.success) {
                    kendo.ui.progress($('#vvveb-builder'), false);
                    alert("success");

                }
                else {
                    kendo.ui.progress($('#vvveb-builder'), false);
                    alert(res.error);
                }
            });
        }

    }
    function OnSaveAs() {
        kendo.ui.progress($('#vvveb-builder'), true);

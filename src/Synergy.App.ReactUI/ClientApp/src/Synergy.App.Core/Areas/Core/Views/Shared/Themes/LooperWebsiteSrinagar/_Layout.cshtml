    function loadDoc(code) {
       
        $.ajax({
             url: '@Url.Action("GetSrinagarSettingWithCode", "EGovernment", new { @area= "EGov" })?code='+code,
                type: 'Get',
                dataType: 'json',
                success: function (result) {
                    //
                    if (result.FileId != undefined && result.FileId!="") {
                        var url2 = '/cms/Document/GetFileMongo?fileId=' + result.FileId;
                        window.open(url2, '_blank');
                    }
                },
                error: function (ert) {
                    //
                    ShowNotification('error');
                }
        });
    }
</script>

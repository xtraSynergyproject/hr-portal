<script type="text/javascript">


    function OnTimelineView() {
        //alert("Timeline");
        $("#divtimelineview").show();
        $("#divgridview").hide();
        $("#ProjectTimeLineGanttChart").data("kendoGantt").dataSource.read();
    }
    function OnGridView() {
        //alert("Grid View");
        $("#divtimelineview").hide();
        $("#divgridview").show();
        GetWBSData();
        //$("#kgdProjectGridView").data("kendoGrid").dataSource.read();

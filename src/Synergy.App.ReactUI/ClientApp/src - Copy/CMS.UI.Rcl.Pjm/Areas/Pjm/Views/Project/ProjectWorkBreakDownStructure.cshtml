        display: block;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    .title {
        font-weight: bold;
        font-size: 13px;
    }

    .resource {
        text-transform: uppercase;
        font-size: 9px;
        margin-top: .5em;
    }

    .progress {
        position: absolute;
        left: 0;
        bottom: 0;
        width: 0%;
        height: 4px;
        background: rgba(0, 0, 0, .3);
    }

    .team-lead-name {
        display: inline-block;
        vertical-align: middle;
        line-height: 32px;
        padding-left: 3px;
    }
    .bnr-img {
        width: 1530px !important;
        padding-left: 15px;
    }
</style>

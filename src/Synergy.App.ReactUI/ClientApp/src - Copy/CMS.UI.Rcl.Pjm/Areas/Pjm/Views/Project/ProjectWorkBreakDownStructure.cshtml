    /*center treelist cell content vertically*/
    .k-gantt .k-treelist td {
        vertical-align: middle;
    }

    /*hide the resource labels, as they are present in the task template*/
    .k-gantt .k-resource {
        display: none;
    }

    /*style the task template*/
    .k-task-template {
        height: 100%;
        padding: 0 !important;
    }

    .template {
        height: 100%;
        overflow: hidden;
    }

    .resource-img {
        float: left;
        width: 32px;
        height: 32px;
        border-radius: 50%;
        margin: 8px;
    }

    .tree-list-img {
        float: left;
        width: 32px;
        height: 32px;
        border-radius: 50%;
    }


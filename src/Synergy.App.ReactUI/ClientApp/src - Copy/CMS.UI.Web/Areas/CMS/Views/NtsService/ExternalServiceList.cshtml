@using CMS.UI.ViewModel;
@using CMS.Common;
@model ServiceViewModel
@{
    ViewBag.Title = "All Service";
    Layout = null;
}
<script type="text/javascript">
    $(document).ready(function () {

        var dataSource = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsService/ReadServiceDataInProgress?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&showAllOwnersService=" + '@ViewBag.ShowAllOwnersService' + "&isDisableCreate="+'@ViewBag.IsDisableCreate',
                    dataType: "json"
                }
            },
        });

        $("#kgrdService").kendoListView({
            dataSource: dataSource,
            dataBound: OnDataBoundPending,
            template: kendo.template($("#templateService").html())
        });

        //Overdue
        var dataSourceOverdue = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsService/ReadServiceDataOverdue?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&showAllOwnersService=" + '@ViewBag.ShowAllOwnersService',
                    dataType: "json"
                }
            },
        });

        $("#kgrdServiceOverDue").kendoListView({
            dataSource: dataSourceOverdue,
            dataBound: OnDataBoundOverdue,
            template: kendo.template($("#templateServiceOverdue").html())
        });

        //Completed
        var dataSourceCom = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsService/ReadServiceDataCompleted?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&showAllOwnersService=" + '@ViewBag.ShowAllOwnersService',
                    dataType: "json"
                }
            },
        });

        $("#kgrdServiceCompleted").kendoListView({
            dataSource: dataSourceCom,
            dataBound: OnDataBoundCompleted,
            template: kendo.template($("#templateServiceCompleted1").html())
        });

        //Accepted
        var dataSourceAcc = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/Cms/NtsService/ReadServiceDataAccepted?moduleCodes=" + '@Model.ModuleCode' + "&templateCodes=" + '@Model.TemplateCode' + "&categoryCodes=" + '@Model.TemplateCategoryCode' + "&showAllOwnersService=" + '@ViewBag.ShowAllOwnersService',
                    dataType: "json"
                }
            },
        });

        $("#kgrdServiceResponseAccepted").kendoListView({
            dataSource: dataSourceAcc,
            dataBound: OnDataBoundAccepted,
            template: kendo.template($("#templateServiceAccepted").html())
        });

    });

    function OnDataBoundPending(arg) {
        document.getElementById('kgrdService').style.border = 'none';
        var data = $("#kgrdService").data("kendoListView").dataSource.data();
        if (data.length == 0) {
            $('#no-data-pending').show();
        } else {
            $('#no-data-pending').hide();
        }
    }

    function OnDataBoundOverdue(arg) {
        document.getElementById('kgrdServiceOverDue').style.border = 'none';
        var data = $("#kgrdServiceOverDue").data("kendoListView").dataSource.data();
        if (data.length == 0) {
            $('#no-data-overdue').show();
        } else {
            $('#no-data-overdue').hide();
        }
    }
    function OnDataBoundCompleted(arg) {
        document.getElementById('kgrdServiceCompleted').style.border = 'none';
        var data = $("#kgrdServiceCompleted").data("kendoListView").dataSource.data();
        if (data.length == 0) {
            $('#no-data-complete').show();
        } else {
            $('#no-data-complete').hide();
        }
    }

    function OnDataBoundAccepted(arg) {
        document.getElementById('kgrdServiceCompleted').style.border = 'none';
        var data = $("#kgrdServiceResponseAccepted").data("kendoListView").dataSource.data();
        if (data.length == 0) {
            $('#no-data-accepted').show();
        } else {
            $('#no-data-accepted').hide();
        }
    }
    var isFiltered = false;

    function createService() {
         var portalId = $('#GlobalPortalId').val();
            var url = '@Url.Action("SelectServiceTemplate", "NtsService",new {area="Cms" })?templateCode=@Model.TemplateCode&categoryCode=@Model.TemplateCategoryCode&userId=' + '@ViewBag.UserId';
            var win = GetMainWindow();
            win.iframeOpenUrl = url;
            win.OpenWindow({ Title: 'Select Service', Width: 1000, Height: 600, Position: 'Right1' });
            return false;
    }

    function OpenService(serviceId, tempCode) {

        var id = serviceId;
        var source = '@RequestSourceEnum.View.ToString()';
        var action = '@DataActionEnum.View.ToString()';

        var portalId = $('#GlobalPortalId').val();

        var url = '/Cms/Page?lo=Popup&viewType=Book&pageType=Service&cbm=OnAfterServiceCreate&source=' + source + '&dataAction=' + action + '&templateCodes=' + tempCode + '&portalId=' + portalId + '&recordId=' + id;
        var win = GetMainWindow();
        win.iframeOpenUrl = url;
        win.OpenWindow({ Title:  'Service', Width: 1200, Height: 600 });
        return false;
    }
    function IframePostbackScript() {
        $("#kgrdService").data("kendoListView").dataSource.read();
        $("#kgrdServiceOverDue").data("kendoListView").dataSource.read();
        $("#kgrdServiceCompleted").data("kendoListView").dataSource.read();
        $("#kgrdServiceResponseAccepted").data("kendoListView").dataSource.read();
    }

    $("#mysearch").keyup(function () {
        var val = $('#mysearch').val();

        var listView = $("#kgrdService").data("kendoListView");
        listView.dataSource.filter({
            logic: "or",
            filters: [
                {
                    field: "ServiceSubject",
                    operator: "contains",
                    value: val
                },
                {
                    field: "ServiceNo",
                    operator: "contains",
                    value: val
                },
                {
                    field: "OwnerDisplayName",
                    operator: "contains",
                    value: val
                },
            ]
        });

        var listView1 = $("#kgrdServiceOverDue").data("kendoListView");
        listView1.dataSource.filter({
            logic: "or",
            filters: [
                {
                    field: "ServiceSubject",
                   operator: "contains",
                    value: val
                },
                {
                    field: "ServiceNo",
                    operator: "contains",
                    value: val
                },
                {
                    field: "OwnerDisplayName",
                    operator: "contains",
                    value: val
                },

            ]
        });


        var listView2 = $("#kgrdServiceCompleted").data("kendoListView");
        listView2.dataSource.filter({
            logic: "or",
            filters: [
                {
                    field: "ServiceSubject",
                    operator: "contains",
                    value: val
                },
                {
                    field: "ServiceNo",
                    operator: "contains",
                    value: val
                },
                {
                    field: "OwnerDisplayName",
                    operator: "contains",
                    value: val
                },
            ]
        });

    var listView2 = $("#kgrdServiceResponseAccepted").data("kendoListView");
        listView2.dataSource.filter({
            logic: "or",
            filters: [
                {
                    field: "ServiceSubject",
                    operator: "contains",
                    value: val
                },
                {
                    field: "ServiceNo",
                    operator: "contains",
                    value: val
                },
                {
                    field: "OwnerDisplayName",
                    operator: "contains",
                    value: val
                },
            ]
        });

    });

    function CloseShare() {

        var win = GetMainWindow();
        win.CloseWindow();
        return false;

    }
    function sortbydate() {
        var listView = $("#kgrdService").data("kendoListView");
        listView.dataSource.sort({ field: "DisplayDueDate", dir: "asc" });
    }
    function sortbyname() {
        var listView = $("#kgrdService").data("kendoListView");
        listView.dataSource.sort({ field: "ServiceSubject", dir: "asc" });
    }

    function sortbyowner() {
        var listView = $("#kgrdService").data("kendoListView");
        listView.dataSource.sort({ field: "OwnerDisplayName", dir: "asc" });
    }


    function filterreset() {
        //resetParam();
        var listView = $("#kgrdService").data("kendoListView");
        //$("#todoheading").html("Task");
        listView.dataSource.filter({});
        listView.dataSource.sort({ field: "CreatedDate", dir: "desc" });
    }
    function OnAfterServiceCreate(task) {
        //alert("TaskLIst");
        $("#kgrdService").data("kendoListView").dataSource.read();
        $("#kgrdServiceOverDue").data("kendoListView").dataSource.read();
        $("#kgrdServiceCompleted").data("kendoListView").dataSource.read();
        $("#kgrdServiceResponseAccepted").data("kendoListView").dataSource.read();
    }
</script>


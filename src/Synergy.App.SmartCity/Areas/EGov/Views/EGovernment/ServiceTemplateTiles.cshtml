    function OnChange(catCode, templateName) {
        debugger;

        if (catCode == "EGOV_NEED_WANT") {
            window.location.href = "/portal/EGovCustomer/CitizenProjectsHome";
            LoadCmsPartialView(window.location.href, 'Custom', false, "", "", "Needs And Wants");
            return false;
        }
        else {
            document.getElementById('categoryName').innerHTML = templateName;
            $("#listViewSL").data('kendoListView').dataSource.read({ categoryCode: catCode });
            $("#templatesDiv").show();
            $("#categoryDiv").hide();
            $('html, body').animate({
                scrollTop: $("#templatesDiv").offset().top - 280
            }, 1000);
        }
    }


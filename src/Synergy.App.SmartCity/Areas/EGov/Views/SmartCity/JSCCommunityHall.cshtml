    function OnDateTypeChange() {
        var type = $('input[name=date_type]:checked').val();
        $('#communityHallDiv').hide();
        if (type == "1") {
            $('#daterangepicker').show();
            $('#calendar').hide();
        } else {
            $('#daterangepicker').hide();
            $('#calendar').show();
        }
        $('#divSubmit').show();
    }
    function OnDateTypeChange() {
        var type = $('input[name=date_type]:checked').val();
        $('#communityHallDiv').hide();
        if (type == "1") {
            $('#daterangepicker').show();
            $('#calendar').hide();
        } else {
            $('#daterangepicker').hide();
            $('#calendar').show();
        }
        $('#divSubmit').show();
    }
    function OnSearchCommHall(e) {
        debugger;
        var dateType;
        var st = new Date();
        var en = new Date();
        var dates = [];
        var type = $('input[name=date_type]:checked').val();
        if (type != null && type != "") {

            if (type == "1") {
                dateType = "DateRange";
                var daterangepicker = $("#daterangepicker").data("kendoDateRangePicker");
                var range = daterangepicker.range();
                if (range != null) {
                    st = new Date(range.start);
                    en = new Date(range.end);

                    var d = new Date(st),
                        month = '' + (d.getMonth() + 1),
                        day = '' + d.getDate(),
                        year = d.getFullYear();

                    if (month.length < 2)
                        month = '0' + month;
                    if (day.length < 2)
                        day = '0' + day;

                    var st = [year, month, day].join('-');

                    var d = new Date(en),
                        day = '' + d.getDate();
                    if (day.length < 2)
                        day = '0' + day;

                    var en = [year, month, day].join('-');

                    $(".text-danger").removeClass("validation-summary-errors");
                    $(".text-danger").addClass("validation-summary-valid");
                    $(".text-danger").html("");

                    GetCommHallList(dateType, st, en, dates);
                }
                else {
                    $(".text-danger").removeClass("validation-summary-valid");
                    $(".text-danger").addClass("validation-summary-errors");
                    $(".text-danger").html("Select dates");
                    e.preventDefault();
                    return false;
                }
            } else {
                dateType = "MultipleDates";
                var calendar = $("#calendar").data("kendoCalendar");
                var value = calendar.selectDates();
                if (value.length > 0) {
                    for (var i = 0; i < value.length; i++) {
                        var d = new Date(value[i]),
                            month = '' + (d.getMonth() + 1),
                            day = '' + d.getDate(),
                            year = d.getFullYear();

                        if (month.length < 2)
                            month = '0' + month;
                        if (day.length < 2)
                            day = '0' + day;

                        var dt = [year, month, day].join('-');

                        //dates.push(value[i].toString());
                        dates.push(dt);
                    }
                    $(".text-danger").removeClass("validation-summary-errors");
                    $(".text-danger").addClass("validation-summary-valid");
                    $(".text-danger").html("");
                    GetCommHallList(dateType, st, en, dates);
                }
                else {
                    $(".text-danger").removeClass("validation-summary-valid");
                    $(".text-danger").addClass("validation-summary-errors");
                    $(".text-danger").html("Select dates");
                    e.preventDefault();
                    return false;
                }
            }
        }
        else {
            $(".text-danger").removeClass("validation-summary-valid");
            $(".text-danger").addClass("validation-summary-errors");
            $(".text-danger").html("Select date type");
            e.preventDefault();
            return false;
        }
    }
    function GetCommHallListLoad() {
        $('#communityHallDiv').hide();
        $('#communityHallDivLoad').show();
        debugger;
        var wid = $("#WardId").data("kendoDropDownList").value();
        var hid = $("#HallId").data("kendoDropDownList").value();

        var parent = document.getElementById('communityHallDivLoad');
        parent.innerHTML = "";
        parent.innerHTML = "<div id='communityHallListLoad'></div>";


        var dataSourceCHLoad = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/EGov/SmartCity/SearchJSCCommunityHallList?communityHallId=" + hid + "&wardId=" + wid,
                    //url: "/egov/smartcity/ReadCommunityHallList?type=" + dateType + "&start=" + st + "&end=" + en + "&dates=" + dates,
                    //data: CommunityHallTemplate,
                    dataType: "json"
                }
            },
        });

        $("#communityHallListLoad").kendoListView({
            dataSource: dataSourceCHLoad,
            layout: "grid",
            template: kendo.template($("#commhalltemplateload").html()),
            dataBound: function () {
                $("#communityHallListLoad").removeClass();
                $("#communityHallListLoad").children('.k-listview-content').removeClass("k-listview-content k-d-grid").addClass("row");
            }
        });
    }

    function GetCommHallList(dateType, st, en, dates) {
        $('#communityHallDivLoad').hide();
        $('#communityHallDiv').show();
        debugger;
        var parent = document.getElementById('communityHallDiv');
        parent.innerHTML = "";
        parent.innerHTML = "<div id='communityHallList'></div>";


        var dataSourceCH = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/egov/smartcity/ReadCommunityHallList?type=" + dateType + "&start=" + st + "&end=" + en + "&dates=" + dates,
                    //data: CommunityHallTemplate,
                    dataType: "json"
                }
            },
        });

        $("#communityHallList").kendoListView({
            dataSource: dataSourceCH,
            layout: "grid",
            template: kendo.template($("#commhalltemplate").html()),
            dataBound: function () {
                $("#communityHallList").removeClass();
                $("#communityHallList").children('.k-listview-content').removeClass("k-listview-content k-d-grid").addClass("row");
            }
        });
    }

    function OnBookNow(hallId, rate) {
        //alert(hallId);
        debugger;
        var dtseltype = "";
        var fdate = "";
        var tdate = "";
        var mdate = "";
        var dates = [];
        var portalId = $("#GlobalPortalId").val();
        var tempCode = "JSC_COMMUNITY_HALL_SERVICE";

        var dttype = $('input[name=date_type]:checked').val();
        if (dttype == "1") {
            dtseltype = '@ViewBag.DateTypeRange';
            var daterangepicker = $("#daterangepicker").data("kendoDateRangePicker");
            var range = daterangepicker.range();
            if (range != null) {
                fdate = kendo.toString(new Date(range.start), 'yyyy-MM-dd'); //'2022-11-11';
                tdate = kendo.toString(new Date(range.end), 'yyyy-MM-dd'); //'2022-11-12';            }
            }
        } else {
            dtseltype = '@ViewBag.DateTypeMulti';
            var calendar = $("#calendar").data("kendoCalendar");
            var value = calendar.selectDates();
            if (value.length > 0) {
                for (var i = 0; i < value.length; i++) {
                    var dt = kendo.toString(new Date(value[i]), 'yyyy-MM-dd');
                    dates.push(dt);
                }
                mdate = dates.toString();
            }
        }
        var prms = encodeURIComponent('ownerUserId=@_userContext.UserId');
        var udfs = encodeURIComponent('Name=@_userContext.Name&BookingById=@ViewBag.BookingBy&DateSelectionType=' + dtseltype + '&BookingFromDate=' + fdate + '&BookingToDate=' + tdate + '&MultipleDates=' + mdate + '&GrandTotal=' + rate + '');
        var url = '/EGov/SmartCity/JSCCommunityHallBooking?hallId=' + hallId + '&bookingById=@ViewBag.BookingBy&dtseltype=' + dtseltype + '&fdate=' + fdate + '&tdate=' + tdate + '&multidate=' + mdate + '&rate=' + rate + '&paymentMode=@ViewBag.PaymentMode';

        //var url = '/Cms/Page?lo=Popup&pageType=Service&cbm=OnAfterServiceCreate&source=Create&dataAction=Create&templateCodes=' + tempCode + '&portalId=' + portalId + '&prms=' + prms + '&udfs=' + udfs;
        var win = GetMainWindow();
        win.iframeOpenUrl = url;
        win.OpenWindow({ Title: "Community Hall Booking", Width: 1000, Height: 700 });
        return false;
    }


    function onGotoPayment(Prms) {
        debugger;
        //alert(Prms);
        //window.location.href = "/portal/JammuSmartCityCustomer/MyPayments";
        // LoadCmsPartialView(window.location.href, 'Custom', false, "", "", "");
        //return false;

        var returnUrl = "/Portal/JammuSmartCityCustomer/MyPayments/";
        //ShowLoader($('#container'));
        $.ajax({
            url: "/EGov/SmartCity/JSCCommunityHallDirectPayment?serviceId=" + Prms,
            dataType: "json",
            success: function (res) {
                if (res.success) {
                    //HideLoader($('#container'));
                    $.ajax({
                        type: "POST",
                        url: "/EGov/SmartCity/OnlinePaymentJSC",
                        data: { 'amount': res.data.Amount, 'ntsType': '@NtsTypeEnum.Task', 'assigneeUserId': res.data.NoteOwnerUserId, 'returnUrl': returnUrl, 'paymentIds': res.data.Id },
                        success: function (resp) {

                            if (resp.success) {
                                if (resp.requestURL === null || resp.requestURL === '' || resp.requestURL === undefined) {
                                    HideLoader($('#container'));
                                    alert('Payment gateway not found');
                                    return false;
                                }
                                else {
                                    window.top.location.href = resp.requestURL;
                                }

                            } else {
                                ShowNotification(resp.error, "error");
                                HideLoader($('#container'));
                            }
                        },
                        dataType: "json",
                    });
                } else {
                    ShowNotification(res.error, "error");
                    //HideLoader($('#container'));
                }
            }

        });
    }

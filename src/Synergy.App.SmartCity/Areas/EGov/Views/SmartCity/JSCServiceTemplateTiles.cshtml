    function OnDateTypeChange() {
        var type = $('input[name=date_type]:checked').val();
        $('#communityHallDiv').hide();
        if (type == "1") {
            $('#daterangepicker').show();
            $('#calendar').hide();
        } else {
            $('#daterangepicker').hide();
            $('#calendar').show();
        }

    }

    function OnSearchCommHall() {
        debugger;
        var dateType;
        var st = new Date();
        var en = new Date();
        var dates = [];
        var type = $('input[name=date_type]:checked').val();
        if (type == "1") {
            dateType = "DateRange";
            var daterangepicker = $("#daterangepicker").data("kendoDateRangePicker");
            var range = daterangepicker.range();
            if (range != null) {
                st = new Date(range.start);
                en = new Date(range.end);

                var d = new Date(st),
                    month = '' + (d.getMonth() + 1),
                    day = '' + d.getDate(),
                    year = d.getFullYear();

                if (month.length < 2)
                    month = '0' + month;
                if (day.length < 2)
                    day = '0' + day;

                var st = [year, month, day].join('-');

                var d = new Date(en),
                    day = '' + d.getDate();
                if (day.length < 2)
                    day = '0' + day;

                var en = [year, month, day].join('-');
            }
        } else {
            dateType = "MultipleDates";
            var calendar = $("#calendar").data("kendoCalendar");
            var value = calendar.selectDates();
            for (var i = 0; i < value.length; i++) {
                var d = new Date(value[i]),
                    month = '' + (d.getMonth() + 1),
                    day = '' + d.getDate(),
                    year = d.getFullYear();

                if (month.length < 2)
                    month = '0' + month;
                if (day.length < 2)
                    day = '0' + day;

                var dt = [year, month, day].join('-');

                //dates.push(value[i].toString());
                dates.push(dt);
            }
        }

        GetCommHallList(dateType, st, en, dates);
    }

    function GetCommHallList(dateType, st, en, dates) {
        $('#communityHallDiv').show();
        debugger;
        var parent = document.getElementById('communityHallDiv');
        parent.innerHTML = "";
        parent.innerHTML = "<div id='communityHallList'></div>";


        var dataSourceCH = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "/egov/smartcity/ReadCommunityHallList?type=" + dateType + "&start=" + st + "&end=" + en + "&dates=" + dates,
                    //data: CommunityHallTemplate,
                    dataType: "json"
                }
            },
        });

        $("#communityHallList").kendoListView({
            dataSource: dataSourceCH,
            layout: "grid",
            template: kendo.template($("#commhalltemplate").html()),
            dataBound: function () {
                $("#communityHallList").removeClass();
                $("#communityHallList").children('.k-listview-content').removeClass("k-listview-content k-d-grid").addClass("row");
            }
        });
    }


    @*<section class="Top-header"></section>*@
   
    <section class="hero-banner-innerpages" style="background-image: url('../../../../Themes/SynergySolutions/img/analytics data.jpg');">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h1 class="hero-banner-title bottom-spacing">Analytics Data</h1><br>
                    <h6 class="hero-banner-subtitle no-display">Manage all your projects and team collaboration at one place</h6>
                </div>

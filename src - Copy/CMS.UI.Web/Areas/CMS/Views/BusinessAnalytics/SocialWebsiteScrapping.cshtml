            startDate:('@ViewBag.KeyWord' != 'null' && '@ViewBag.KeyWord' != '') ? null : $("#startDate").val(),
            endDate:('@ViewBag.KeyWord' != 'null' && '@ViewBag.KeyWord' != '') ? null : $("#endDate").val()
        };
        return search;
    }
    function FilterAdv() {
        var search = {

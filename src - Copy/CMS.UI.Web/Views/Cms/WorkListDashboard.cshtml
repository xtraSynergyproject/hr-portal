    function OnAfterServiceCreate(service) {
        location.href = location.href;
    }
    function OnAfterTaskCreate(task) {
        location.href = location.href;
    }
    function OnAfterNoteCreate(task) {
        location.href = location.href;
    }
    function OnService(mode, status) {
        var url = "/cms/NtsService/ServiceHome?serviceStatus=" + status + "&mode=" + mode+"&lo=Popup&moduleCodes=@Model.ModuleCodes&userId=@Model.UserId";
        var win = GetMainWindow();
        //var url = '/Cms/Page?popup=true&lo=Popup&pageType=Custom&source=Main&dataAction=None&pageName=ServiceHome&portalId=' + portalId /*+ '&prms=' + prms*/;
        win.iframeOpenUrl = url;

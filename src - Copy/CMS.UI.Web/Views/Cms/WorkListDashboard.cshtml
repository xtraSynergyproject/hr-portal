<script>

    function OnFilterChange(e) {
        var dataItem = e.sender.dataItem();
        if (dataItem.Value == "1") {
            $("#TeamFilter").hide();
            $("#ProjFilter").show();
        }
        if (dataItem.Value == "2") {
            $("#TeamFilter").show();
            $("#ProjFilter").hide();
        }
        SubordinateCount();
    }
    function OnTypeBound(e) {
        var dataItem = e.sender.dataItem();
        if (dataItem.Value == "1") {
            $("#TeamFilter").hide();
            $("#ProjFilter").show();
        }
        if (dataItem.Value == "2") {
            $("#TeamFilter").show();
            $("#ProjFilter").hide();
        }

    }
    function OnProjectChange(e) {

        SubordinateCount();
    }
    function OnMemberChange(e) {

        SubordinateCount();
    }
    function OnTeamChanges(e) {

        SubordinateCount();
    }

    function FilterUser() {
        return { teamId: $("#Team").data("kendoDropDownList").value() }
    }
    function FilterMembers() {
        return { projectId: $("#Projects").data("kendoDropDownList").value() }
    }
    function OnServiceCreate() {
        var portalId = $('#GlobalPortalId').val();
        //var prms = encodeURIComponent( 'parentServiceId=123&parentTaskId=223');

        var url = '@Url.Action("SelectServiceTemplate", "NtsService",new {area="Cms" })?templateCode=&categoryCode=&moduleCodes=@Model.ModuleCodes&userId=@Model.UserId';//'/Cms/Page?lo=Popup&cbm=OnAfterServiceCreate&source=Create&dataAction=Create&pageName=Project&portalId=' + portalId /*+ '&prms=' + prms*/;

        var win = GetMainWindow();
        win.iframeOpenUrl = url;
        win.OpenWindow({ Title: '@Html.Raw(SharedResource["CreateService"])', Width: 1000, Height: 600, Position: 'Right1' });
        return false;
    }
    function OnTaskCreate() {
        var portalId = $('#GlobalPortalId').val();

        var url = '@Url.Action("SelectTaskTemplate", "NtsTask",new {area="Cms" })?templateCode=&categoryCode=&moduleCodes=@Model.ModuleCodes&userId=@Model.UserId';//'/Cms/Page?lo=Popup&cbm=OnAfterServiceCreate&source=Create&dataAction=Create&pageName=Project&portalId=' + portalId /*+ '&prms='Model+ prms*/;

        var win = GetMainWindow();
        win.iframeOpenUrl = url;
        win.OpenWindow({ Title: '@Html.Raw(SharedResource["CreateTask"])', Width: 1000, Height: 600 });
        return false;
    }
    function OnNoteCreate() {
        var portalId = $('#GlobalPortalId').val();
        //var prms = encodeURIComponent( 'parentServiceId=123&parentTaskId=223');


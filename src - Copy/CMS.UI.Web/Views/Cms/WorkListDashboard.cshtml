    function OnNote(mode, status) {
        var url = "/cms/NtsNote/NoteHome?noteStatus=" + status + "&mode=" + mode + "&lo=Popup&moduleCodes=@Model.ModuleCodes&userId=@Model.UserId";
        var win = GetMainWindow();
